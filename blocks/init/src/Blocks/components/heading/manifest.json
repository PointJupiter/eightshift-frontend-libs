{
	"componentName": "heading",
	"title": "Heading",
	"componentClass": "heading",
	"example": {
		"attributes": {
			"headingContent": "This is heading",
			"headingLevel": 2,
			"headingSize": "default",
			"headingColor": "black",
			"headingUse": true
		}
	},
	"attributes": {
		"headingContent": {
			"type": "string",
			"seo": true
		},
		"headingLevel": {
			"type": "integer",
			"default": 2
		},
		"headingSize": {
			"type": "string",
			"default": "default"
		},
		"headingColor": {
			"type": "string",
			"default": "black"
		},
		"headingUse": {
			"type": "boolean",
			"default": true
		}
	},
	"options": {
		"headingSize": [
			{
				"label": "Default (1.5em)",
				"value": "default"
			},
			{
				"label": "Big (2.5em)",
				"value": "big"
			}
		],
		"headingColor": [
			"primary",
			"black"
		]
	},
	"variables": {
		"headingSize": {
			"default": [
				{
					"variable": {
						"heading-font-size": "1em",
						"heading-line-height": "1"
					}
				},
				{
					"breakpoint": "tablet",
					"variable": {
						"heading-font-size": "1.5em"
					}
				}
			],
			"big": [
				{
					"variable": {
						"heading-font-size": "1.5em",
						"heading-line-height": "1"
					}
				},
				{
					"breakpoint": "tablet",
					"variable": {
						"heading-font-size": "2.5em"
					}
				}
			]
		},
		"headingColor": [
			{
				"variable": {
					"heading-color": "var(--global-colors-%value%)"
				}
			}
		]
	}
}
