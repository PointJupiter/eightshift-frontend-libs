    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
