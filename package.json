    "@wordpress/i18n": "3.12.0",
    "@wordpress/icons": "^2.0.0",
    "@wordpress/server-side-render": "^1.12.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
