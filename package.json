    "node-emoji": "^1.10.0",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "promisify-child-process": "^3.1.4",
    "raw-loader": "^4.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "swiper": "^5.3.8",
    "terser-webpack-plugin": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "jest": "^25.5.4",
    "replace-in-file": "^6.0.0",
    "sassdoc": "^2.7.1"
  },
  "sideEffects": false
}
