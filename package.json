  "scripts": {
    "__eslintTheme": "eslint '**/*.js'",
    "__stylelintTheme": "stylelint **/*.scss",
    "lintStyle": "npm run __stylelintTheme",
    "lintJs": "npm run __eslintTheme",
    "lint": "npm run lintJs && npm run lintStyle",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs/storybook",
    "build-sassdoc": "node ./node_modules/sassdoc/bin/sassdoc styles/scss --dest docs/sassdoc",
    "build-docs": "npm run build-sassdoc && npm run build-storybook",
    "deploy-docs": "storybook-to-ghpages"
  },
